{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "_": true,
    "Backbone": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
